<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <update handle="empty"/>
    <referenceContainer name="page.wrapper">
        <container name="header.container" as="header_container" label="Page Header Container" htmlTag="header" htmlClass="page-header" before="main.content"/>
        <container name="page.top.wrap" as="" label="After Page Header Wrap" htmlTag="div" htmlClass="header-bottom-wrap" after="header.container">
            <container name="page.top" as="page_top" label="After Page Header" htmlTag="div" htmlClass="header-bottom"/>
        </container>
        <container name="breadcrumbs.wrap" label="Breadcrumbs wrap container" after="header.container" />
        <container name="footer-container" as="footer" before="before.body.end" label="Page Footer Container" htmlTag="footer" htmlClass="page-footer">
            <container name="footer-copyright" after="-" htmlTag="div" htmlClass="footer-copyright" />
        </container>
    </referenceContainer>
</layout>
